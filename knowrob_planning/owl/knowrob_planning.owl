<?xml version="1.0"?>
<rdf:RDF xmlns="http://knowrob.org/kb/knowrob_planning.owl#"
     xml:base="http://knowrob.org/kb/knowrob_planning.owl"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:knowrob_planning="http://knowrob.org/kb/knowrob_planning.owl#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:knowrob="http://knowrob.org/kb/knowrob.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:computable="http://knowrob.org/kb/computable.owl#">
    <owl:Ontology rdf:about="http://knowrob.org/kb/knowrob_planning.owl">
        <owl:imports rdf:resource="package://knowrob_common/owl/knowrob.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#guides -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/knowrob_planning.owl#guides">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Workflow"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#hasFirstStep -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/knowrob_planning.owl#hasFirstStep">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#hasStep"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Workflow"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:label>has first step</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#hasGoal -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/knowrob_planning.owl#hasGoal">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasComponent"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal"/>
        <rdfs:label>has goal</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#hasNextStep -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/knowrob_planning.owl#hasNextStep">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#directlyPrecedes"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:label>has next step</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#hasSalientTask -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/knowrob_planning.owl#hasSalientTask">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#usesConcept"/>
        <owl:inverseOf rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#isSalientTaskIn"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:label>has salient task</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#hasStep -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/knowrob_planning.owl#hasStep">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#definesTask"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Workflow"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:label>has step</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#isSalientTaskIn -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/knowrob_planning.owl#isSalientTaskIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isConceptUsedIn"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
        <rdfs:label>is salient task in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#hasInputParameter -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#hasInputParameter"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#concretelyExpresses -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#concretelyExpresses"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#definesRole -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#definesRole"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#definesTask -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#definesTask"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#directlyPrecedes -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#directlyPrecedes"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasComponent -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasComponent"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParameter -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParameter"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isAbout -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isAbout"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isConceptUsedIn -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isConceptUsedIn"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskOf -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskOf"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#realizes -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#realizes"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#usesConcept -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#usesConcept"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#hasCardinalityValue -->

    <owl:DatatypeProperty rdf:about="http://knowrob.org/kb/knowrob_planning.owl#hasCardinalityValue">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasDataValue"/>
        <rdfs:domain rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#KBTaskItem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:label>has cardinality value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#hasDepthOfSearchValue -->

    <owl:DatatypeProperty rdf:about="http://knowrob.org/kb/knowrob_planning.owl#hasDepthOfSearchValue">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasDataValue"/>
        <rdfs:domain rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#KBTaskItem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:label>has depth of search value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#hasInhibitionValue -->

    <owl:DatatypeProperty rdf:about="http://knowrob.org/kb/knowrob_planning.owl#hasInhibitionValue">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasDataValue"/>
        <rdfs:domain rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#KBTaskItem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:label>has inhibition value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#hasPriorityValue -->

    <owl:DatatypeProperty rdf:about="http://knowrob.org/kb/knowrob_planning.owl#hasPriorityValue">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasDataValue"/>
        <rdfs:domain rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#KBTaskItem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>has priority value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasDataValue -->

    <owl:DatatypeProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasDataValue"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#Agenda -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#Agenda">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl#DataStructure"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses"/>
                <owl:someValuesFrom rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#GroundingPlan"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isAbout"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart"/>
                <owl:allValuesFrom rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#KBTaskItem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A priority queue DataStructure with KBTask parts.</rdfs:comment>
        <rdfs:comment>todo: not so sure about *expresses*</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#AttentionControl -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#AttentionControl">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#AttentionPlan -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#AttentionPlan">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#MetaPlan"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#hasGoal"/>
                <owl:someValuesFrom rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#AttentionControl"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#hasSalientTask"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#definesRole"/>
                <owl:someValuesFrom rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#Censor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#definesTask"/>
                <owl:someValuesFrom rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#BeingAttentive"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#hasSalientTask"/>
                <owl:allValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A Plan that denotes which possible steps are salient when some other Plan is executed.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#BeingAttentive -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#BeingAttentive">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskOf"/>
                <owl:someValuesFrom rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#Censor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A Task in which a Censor filters out things.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#Censor -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#Censor">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies"/>
                <owl:allValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The role an Agent takes when it censors things.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#Censored -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#Censored">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#Classification -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#Classification">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#KBTask"/>
        <owl:disjointWith rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#Nullification"/>
        <owl:disjointWith rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#Parametrization"/>
        <rdfs:comment>A KBTask where the type of the Subject needs to be specialized.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#Constraint -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#Constraint">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#ContinuityStrategy -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#ContinuityStrategy">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#SelectionCriterion"/>
        <rdfs:comment>Prefer to stick to the subject for which a KBTask was executed in the last step. The selection value is 1 if the subject is the same as last step, and 0 otherwise.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#Decomposition -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#Decomposition">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#Parametrization"/>
        <owl:disjointWith rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#Integration"/>
        <rdfs:comment>A KBTask where some relation needs to be decomposed by grounding new entity symbols that relate to the Subject.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#DepthFirstStrategy -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#DepthFirstStrategy">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#SelectionCriterion"/>
        <rdfs:comment>Prefer KBTask with higher depth in the ABox graph spanned by the root object.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#EntityGrounding -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#EntityGrounding">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart"/>
                <owl:allValuesFrom rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#KBTask"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskOf"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#Subject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A Task in which some Entity is the Subject of discourse, and where some facts about this Entity are to be modified in the knowledge base.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#FailureCode -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#FailureCode">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Parameter"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#FailureRecovery -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#FailureRecovery">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#hasInputParameter"/>
                <owl:someValuesFrom rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#FailureCode"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskOf"/>
                <owl:someValuesFrom rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#Troubleshooter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A Task in which a Troubleshooter has to recover from a failure.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#GroundedEntity -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#GroundedEntity">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#Grounder -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#Grounder">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies"/>
                <owl:allValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The role an Agent takes when grounding an Entity.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob_planning.owl#GroundingPlan -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob_planning.owl#GroundingPlan">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#MetaPlan"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#hasGoal"/>
                <owl:someValuesFrom rdf:resource="http://knowrob.org/kb/knowrob_planning.owl#GroundedEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
